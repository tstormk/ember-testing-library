{
  "name": "ember-testing-library",
  "version": "1.0.4",
  "description": "Simple and complete Ember DOM testing utilities that encourage good testing practices.",
  "keywords": [
    "ember-addon",
    "testing",
    "ui",
    "dom",
    "unit",
    "integration",
    "functional",
    "end-to-end",
    "e2e"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:tstormk/ember-testing-library.git"
  },
  "license": "MIT",
  "author": "Theodor Storm <tstormk@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint:types": "tsc --noEmit",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "release": "release-it"
  },
  "dependencies": {
    "@testing-library/dom": "^8.1.0",
    "broccoli-funnel": "^3.0.6",
    "ember-auto-import": "^1.10.1",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-typescript": "^4.2.1"
  },
  "devDependencies": {
    "@clark/ember-template-lint-config": "^0.2.2",
    "@clark/eslint-config-ember": "^2.2.0",
    "@clark/eslint-config-ember-typescript": "^2.2.0",
    "@clark/eslint-config-node": "^2.2.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.8",
    "@types/ember": "^3.16.5",
    "@types/ember-qunit": "^3.4.14",
    "@types/ember-resolver": "^5.0.10",
    "@types/ember__application": "^3.16.3",
    "@types/ember__array": "^3.16.4",
    "@types/ember__component": "^3.16.6",
    "@types/ember__controller": "^3.16.5",
    "@types/ember__debug": "^3.16.4",
    "@types/ember__engine": "^3.16.3",
    "@types/ember__error": "^3.16.1",
    "@types/ember__object": "^3.12.6",
    "@types/ember__polyfills": "^3.12.1",
    "@types/ember__routing": "^3.16.15",
    "@types/ember__runloop": "^3.16.3",
    "@types/ember__service": "^3.16.1",
    "@types/ember__string": "^3.16.3",
    "@types/ember__template": "^3.16.1",
    "@types/ember__test": "^3.16.1",
    "@types/ember__test-helpers": "^2.0.1",
    "@types/ember__utils": "^3.16.2",
    "@types/htmlbars-inline-precompile": "^1.0.1",
    "@types/qunit": "^2.11.2",
    "@types/rsvp": "^4.0.4",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.24.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-dependency-lint": "^2.0.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.25.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.18.1",
    "ember-try": "^1.4.0",
    "eslint": "^7.29.0",
    "loader.js": "^4.7.0",
    "qunit": "^2.16.0",
    "release-it": "^14.10.0",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">= 10.*"
  },
  "publishConfig": {
    "access": "public"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    }
  }
}
